# Docker Compose Configuration
COMPOSE_PROJECT_NAME=tda-geo

# Jupyter Configuration
# Set to blank for no token (local dev only - NOT for production!)
# For security, set a strong token: JUPYTER_TOKEN=your-secret-token-here
JUPYTER_TOKEN=

# Use JupyterLab interface (yes) or classic Notebook (no)
JUPYTER_ENABLE_LAB=yes

# Port Mappings (change if you have conflicts)
JUPYTER_PORT=8888
DASK_DASHBOARD_PORT=8787
TENSORBOARD_PORT=6006

# Optional: Run as root in container (not recommended, but sometimes needed for debugging)
# RUN_AS_ROOT=0
